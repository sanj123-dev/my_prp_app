{
  "version": 2,
  "builds": [
    {
      "src": "backend.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": "{myenv/**,__pycache__/**,src/**/__pycache__/**,.env,*.pyc,.pytest_cache/**}"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend.py"
    }
  ]
}
