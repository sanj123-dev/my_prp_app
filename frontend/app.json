{
    "expo":  {
                 "name":  "finTracker",
                 "slug":  "frontend",
                 "version":  "1.0.0",
                 "orientation":  "portrait",
                 "icon":  "./assets/images/icon.png",
                 "scheme":  "frontend",
                 "userInterfaceStyle":  "automatic",
                 "newArchEnabled":  true,
                 "ios":  {
                             "supportsTablet":  true,
                             "infoPlist":  {
                                               "NSContactsUsageDescription":  "Read transaction messages to track spending",
                                               "NSSMSUsageDescription":  "Read SMS to auto-categorize transactions",
                                               "NSMicrophoneUsageDescription": "Allow microphone access to talk with your virtual assistant",
                                               "NSSpeechRecognitionUsageDescription": "Allow speech recognition so your assistant can understand your voice"
                                           }
                         },
                 "android":  {
                                 "adaptiveIcon":  {
                                                      "foregroundImage":  "./assets/images/adaptive-icon.png",
                                                      "backgroundColor":  "#000"
                                                  },
                                 "edgeToEdgeEnabled":  true,
                                 "permissions":  [
                                                     "READ_SMS",
                                                     "RECEIVE_SMS",
                                                     "READ_SMS",
                                                     "RECEIVE_SMS",
                                                     "RECORD_AUDIO"
                                                 ],
                                 "package":  "com.anonymous.frontend"
                             },
                 "web":  {
                             "bundler":  "metro",
                             "output":  "static",
                             "favicon":  "./assets/images/favicon.png"
                         },
                 "plugins":  [
                                 "expo-router",
                                 "expo-speech-recognition",
                                 [
                                     "expo-splash-screen",
                                     {
                                         "image":  "./assets/images/splash-image.png",
                                         "imageWidth":  200,
                                         "resizeMode":  "contain",
                                         "backgroundColor":  "#000"
                                     }
                                 ]
                             ],
                 "experiments":  {
                                     "typedRoutes":  true
                                 },
                 "extra":  {
                               "router":  {

                                          },
                               "eas":  {
                                           "projectId":  "4f5baf15-b74c-4db6-920b-82cb02357b32"
                                       }
                           }
             }
}
